{
  "name": "luciadmapcontrols",
  "version": "1.0.0",
  "description": "Set of map controls compatible with LuciadRIA 2020.0 release",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm i symlink-dir -D && npm i mkdirp -D && npm i del-symlinks -D",
    "postinstall": "mkdirp ./node_modules/@luciad && symlink-dir ../@luciad/ria ./node_modules/@luciad/ria",
    "preuninstall": "del-symlinks ./node_modules/@luciad/ria"
  },
  "keywords": [
    "luciad",
    "layercontrol",
    "map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciaddeveloperplatform/luciadmapcontrols"
  },
  "author": "Hegaxon",
  "license": "MIT",
  "peerDependencies": {
    "@luciad/ria": "^2020.0",
    "bootstrap-sass": ">= 3.4.1"
  },
  "devDependencies": {}
}
